{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
  ],
  "dependencyDashboard": false,
  "stabilityDays": 2,
  "addLabels": [
    "dependencies",
  ],
  "prConcurrentLimit": 3,
  "schedule": [
    "on the 5th day of the month every 1 month at 09:00",
  ],
  "timezone": "Asia/Jerusalem",
  "pre-commit": {
    "enabled": true,
  },
}
